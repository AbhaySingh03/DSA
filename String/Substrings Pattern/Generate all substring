I/P = S = "abc"

O/P =  a ab abc  b bc  c

class Main {
    public static void main(String[] args) {
        String s ="abc";
       
        for(int i=0;i<s.length();i++){
            for(int j=i;j<=s.length();j++){
                 System.out.print(s.substring(i,j)+" ");
            }
        }
    }
}
